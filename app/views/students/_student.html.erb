<%= form_with model: @student do |form|%>
    <%= form.label :Name%>
    <%= form.text_field :name%>
    <%= form.label :CPF%>
    <%= form.text_field :cpf%>
    <%= form.label :Birth%>
    <%= form.date_field :date_of_birth%>
    <%= form.label :Cellphone%>
    <%= form.telephone_field :cellphone%>
    <%= form.label :Gender%>
    <%= form.select :gender, Student.genders.keys%>
    <%= form.label :Payment%>
    <%= form.select :payment_method, Student.payment_methods.keys%>
    <%= form.submit "Save" %>
<% end %>