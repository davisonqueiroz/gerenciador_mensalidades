<%= form_with model: @invoice do |form|%>
    <%= form.label :invoice_value%>
    <%= form.text_field :invoice_value%>
    <%= form.label :due_date%>
    <%= form.date_field :due_date%>
    <%= form.label :enrollment%>
    <%= form.collection_select :enrollment_id, Enrollment.all,:id,:id,prompt: true%>
    <%= form.label :status%>
    <%= form.select :status,[['Aberta', 'aberta'], ['Atrasada', 'atrasada'], ['Paga', 'paga']],prompt: true%>
    <%= form.submit "Save" %>
<% end %>